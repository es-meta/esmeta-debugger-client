{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
			"@/*": ["./src/*"],
			"@esmeta/*": ["./scalajs/target/scala-3.3.3/esmeta-worker-opt/*"],
			"@esmeta-debug/*": ["./scalajs/target/scala-3.3.3/esmeta-worker-fastopt/*"],
		}
  },
  "include": [
    "src"
  ]
}
